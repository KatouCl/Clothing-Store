@model KS.ViewModels.FeedBack.FeedBackIndexVm

@{
    ViewBag.Title = "title";
    Layout = "_AdminLayout";
}


<div class="container">
    <div class="row">
        <div class="col-12">
            <div class="tab">
                <div class="tab-content">
                    <div class="tab-pane active" id="tab-1" role="tabpanel">
                        <div class="row pt-5">
                            <div class="col-md-10">
                                <div class="mb-3 row">
                                    <label asp-for="Name" class="col-form-label col-sm-2 text-sm-end">Название</label>
                                    <div class="col-sm-10">
                                        <input asp-for="Name" class="form-control" rows="3" disabled/>
                                    </div>
                                </div>
                                <div class="mb-3 row">
                                    <label asp-for="Email" class="col-form-label col-sm-2 text-sm-end">Почта</label>
                                    <div class="col-sm-10">
                                        <input asp-for="Email" class="form-control" rows="3" disabled/>
                                    </div>
                                </div>
                                <div class="mb-3 row">
                                    <label asp-for="Comment" class="col-form-label col-sm-2 text-sm-end">Содержание</label>
                                    <div class="col-sm-10">
                                        <textarea id="ContentShortDescription" asp-for="Comment" class="form-control" rows="3" disabled></textarea>
                                    </div>
                                </div>
                                <div class="mb-3 row">
                                    <label asp-for="CreationDate" class="col-form-label col-sm-2 text-sm-end">Дата создания </label>
                                    <div class="col-sm-10">
                                        <textarea id="ContentDescription" asp-for="CreationDate" class="form-control" rows="3" disabled></textarea>
                                    </div>
                                </div>
                                <hr>
                                <a class="float-left float-end mt-n1" asp-area="Admin" asp-controller="FeedBack" asp-action="Index">Вернуться </a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>